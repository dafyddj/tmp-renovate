{
  "enabledManagers": [
    "custom.regex",
  ],
  "allowedPostUpgradeCommands": ["echo Working",],
  "branchPrefix": "gha-renovate/",
  "requireConfig": "ignored",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^.copier-answers.yml$"],
      "matchStrings": [
        "_commit: (?<currentValue>\\S+)\\n_src_path: gh:(?<depName>\\S+)\\n",
      ],
      "datasourceTemplate": "github-tags",
    },
  ],
  "postUpgradeTasks": {
    "commands": ["echo Working",],
    "executionMode": "update"
  },
}
