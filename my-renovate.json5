{
  "enabledManagers": [
    "custom.regex",
  ],
  "allowedPostUpgradeCommands": ["copier update -A",],
  "branchPrefix": "gha-renovate1/",
  "requireConfig": "ignored",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^.copier-answers.yml$"],
      "matchStrings": [
        "_commit: (?<currentValue>\\S+)\\n_src_path: gh:(?<depName>\\S+)\\n",
      ],
      "autoReplaceStringTemplate": "_commit: {{{currentValue}}} # __copier_update_pending {{{newValue}}}\n_src_path: gh:{{{depName}}}\n",
      "datasourceTemplate": "github-tags",
    },
  ],
  "postUpgradeTasks": {
    "commands": ["copier update -A",],
    "executionMode": "update"
  },
  "semanticCommitScope": "1",
}
